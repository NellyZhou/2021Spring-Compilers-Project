int fact(int n) 
{
    if (n == 1){
        return n;
    }
    else{
        int t;
        write(n);
        t = fact(n - 1);
        write(t);
        return (n * t); 
    }
}

int main()
{
    int m, result;
    m = read();
    if (m > 1)
        result = fact(m);
    else
        result = 1;
    write(result);
    return 0;
}